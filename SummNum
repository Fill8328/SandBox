public class sumchisel {
    public static void main(String []args){
        int x=1234056789;
        int s=0;
        int a=0;
        for(;x>0;){
            a=x%10;
            System.out.println("a="+a);
            x=x/10;
            System.out.println("x="+x);
            s=s+a;
            System.out.println("s="+s);
        }
    }
}
